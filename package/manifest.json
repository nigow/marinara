{
  "name": "__MSG_app_name__",
  "short_name": "__MSG_app_name_short__",
  "description": "__MSG_app_desc__",
  "default_locale": "en",
  "version": "35",
  "manifest_version": 3,
  "author": "Chris Schmich <schmch@gmail.com>",
  "homepage_url": "https://github.com/schmich/marinara",
  "offline_enabled": true,
  "permissions": [
    "alarms",
    "contextMenus",
    "storage",
    "notifications",
    "tabs",
    "windows",
    "system.display"
  ],
  "action": {
    "default_title": "Marinara",
    "default_icon": "images/browser-action.png"
  },
  "background": {
    "service_worker": "modules/background-sw.js"
  },
  "options_page": "modules/options.html",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "minimum_chrome_version": "55",
  "content_security_policy": {
    "extension_pages": "script-src 'self'"
  }
}
